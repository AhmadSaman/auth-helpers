{
  "extends": "./.svelte-kit/tsconfig.json",
  "include": ["src"],
  "compilerOptions": {
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "paths": {
        "$lib":["src"],
        "$lib/*":["src/*"]
    }
  }
}
